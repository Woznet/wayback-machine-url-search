{
	"manifest_version": 3,
	"version": "1.0",
	"name": "Wayback Machine URL Search",
	"description": "Search the Internet Achieve Wayback Machine for a URL or hyperlink",
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"contextMenus"
	],
	"icons": {
		"16": "icons/favicon16.png",
		"32": "icons/favicon32.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	}
}